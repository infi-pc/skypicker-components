.search-place-picker
  .content
    padding-bottom: 6px;
    .places
      overflow-y scroll
      height: 100%;
      width: 417px;
  .place-row
    height 30px;
    line-height 30px;
    width: 390px
    padding-top 0px
    border-bottom: $border
    cursor pointer
    &:hover, &.selected
      background-color #fff9ce
    .name
      float left
      width: 300px
    .type
      float right

