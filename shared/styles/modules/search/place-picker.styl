.search-place-picker
  .content
    padding-bottom: 6px;
    .places
      overflow-y scroll
      height: 100%;
      width: 417px;

  .place-row
    position: relative
    height 30px;
    line-height 30px;
    width: 390px
    padding-top 0px
    border-bottom: $border
    cursor pointer
    &:hover, &.selected
      background-color #fff9ce

    .name
      position: absolute;
      /*margin-top: 11px;*/

    .type
      position: absolute;
      right: 0px

