
.places-overlay
  position: relative;

.places-overlay > div
  position: absolute;


.city-label
  margin-left: -7px;
  margin-top: -7px;
  z-index: 100;
  cursor: pointer
  text-shadow: 0px 0px 1px #F1EFE8, 0px 0px 1px #F1EFE8, 0px 0px 2px #F1EFE8;

.city-label > img
  z-index: 101;

.city-label > div
  position: absolute;
  top: -8px;
  left: 15px;
  white-space: nowrap;
  z-index: 102;

.city-label .city-label-title
  font-weight: bold;
  color: #333;

.city-label-price
  font-size: 11px


.city-label.flag-origin .city-label-title, .city-label.flag-destination .city-label-title
  color: #000;
  font-size: 16px;
  /*text-shadow: -2px 2px 0px #fff, 2px -2px 0px #fff, 2px 2px 0px #fff, -2px -2px 0px #fff;*/


#output
  position: absolute;
  top: 20px;
  background-color: #fff;

